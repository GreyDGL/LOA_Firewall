# LLM Firewall Environment Configuration
# Copy this file to .env and update with your values

# License Configuration
LLM_FIREWALL_SECRET=your-master-secret-key-here
LLM_FIREWALL_LICENSE=./license.key

# API Configuration
LLM_FIREWALL_HOST=0.0.0.0
LLM_FIREWALL_PORT=5001
LLM_FIREWALL_CONFIG=./config.json

# Logging Configuration
LLM_FIREWALL_LOG_LEVEL=INFO
LLM_FIREWALL_DEBUG=false

# Ollama Configuration (if using local Ollama)
OLLAMA_HOST=0.0.0.0:11434

# Additional Security (optional)
# FIREWALL_API_KEY=your-api-key-for-auth
# FIREWALL_RATE_LIMIT=100